<%= form_for(@resume, :url => (@resume.new_record? ? resumes_path : resume_path(@resume)), :method => (@resume.new_record? ? "POST" : "PUT"), :html => {:multipart => true, :id => 'user-form'}) do |f| %>
  <%= hidden_field_tag "step", @step.to_i %>
  <div class="row">
    <div class="col-12 col-sm-9">
      <div class="items-left wb-rounded">
        <div class="item-header">
          <div class="row mx-n2">
            <div class="col-12">
              <div class="text-center"><h4>Project Experience</h4></div>
                <div id="projects">
                <%= f.fields_for :projects do |project| %>
                    <%= render 'project_fields', f: project %>
                <% end %>
                <div class="text-right">
                  <%= link_to_add_association 'Add projects', f, :projects, partial: 'project_fields', class: 'add-buttons pull-right' %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <div class="col-12 col-sm-3">
      <div class="items-btns">
        <a class="btn btn-primary" href="<%= edit_resume_path(@resume, step: 1) %>">
          <span class="icon-close-back">return</span>
        </a>
        <button class="btn btn-primary" id="saveMedia" name="save" type="submit">
          <span class="icon-next">save</span>
        </button>
        <button class="btn btn-primary" name="proceed_next" type="submit">
          <br class="d-none d-md-block">
          </br>
          <span class="icon-next">save and next</span>
        </button>
      </div>
    </div>
  </div>
<% end %>
        




    