<section class="section-box">
  <div class="container">
    <div class="row row-md-17">
      <div class="col-12 col-sm-6 col-lg-4">
        <a class="items-box" href="<%= resume_path(@resume, :type => "Normal") %>">
          <div class="item-icon">
            <%= image_tag  "icon-user.svg" %>
          </div>
          <div class="item-name">
            <%= "BASCIS" %>
          </div>
        </a>
      </div>
      <div class="col-12 col-sm-6 col-lg-4">
        <a class="items-box" href="<%= resume_path(@resume, :type => "Intermediate") %>">
          <div class="item-icon">
            <%= image_tag  "icon-prepare-meeting.svg", :width => "50" %>
          </div>
          <div class="item-name">
            <%= "Intermediate" %>
          </div>
        </a>
      </div>
      <div class="col-12 col-sm-6 col-lg-4">
        <a class="items-box" href="<%= resume_path(@resume, :type => "Advanced") %>">
          <div class="item-icon">
            <%= image_tag  "icon-customer-message.svg" %>
          </div>
          <div class="item-name">
            <%= "Advanced" %>
          </div>
